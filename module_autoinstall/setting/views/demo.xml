<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="res_config_setting_inherit" model="ir.ui.view">
        <field name="name">setting modification</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="sale.res_config_settings_view_form" />
        <field name="arch" type="xml">
          <div id="delivery" position="after">
            <div class="col-xs-12 col-md-6 o_setting_box" id="delivery_method">
                <div class="o_setting_left_pane">
                    <field name="shipping_type"/>
                </div>
                <div class="o_setting_right_pane">
                    <label for="shipping_type"/>
                    <div class="text-muted" id="delivery_meth">
                        Activate Delivery Method
                    </div>
                </div>
            </div>
          </div>
        </field>
    </record>
</odoo>



